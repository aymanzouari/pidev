<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.ArticleController">
   <children>
      <Label layoutX="42.0" layoutY="31.0" text="LISTE DES ARTICLES">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tablearticle" layoutX="1.0" layoutY="84.0" onKeyPressed="#articlee" prefHeight="404.0" prefWidth="745.0">
        <columns>
          <TableColumn fx:id="idnom" prefWidth="111.0" text="Nom" />
          <TableColumn fx:id="idimage" prefWidth="136.0" text="Image" />
            <TableColumn fx:id="iddescription" minWidth="0.0" prefWidth="349.0" text="Description" />
            <TableColumn fx:id="iddate" prefWidth="153.0" text="Datearticle" />
        </columns>
      </TableView>
      <Button layoutX="164.0" layoutY="506.0" mnemonicParsing="false" onAction="#ajoutearticle" prefHeight="25.0" prefWidth="93.0" text="Ajouter" />
      <Button layoutX="427.0" layoutY="506.0" mnemonicParsing="false" onAction="#supprimerarticle" prefHeight="25.0" prefWidth="93.0" text="Supprimer" />
      <Button layoutX="295.0" layoutY="506.0" mnemonicParsing="false" onAction="#modifiearticle" prefHeight="25.0" prefWidth="93.0" text="Modifier" />
   </children>
</AnchorPane>
