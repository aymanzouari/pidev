      <TextField fx:id="idcategorie" layoutX="142.0" layoutY="69.0" prefHeight="25.0" prefWidth="492.0" style="-fx-background-color: transparant; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: black;" />
      <Button fx:id="ajoutercategorie" layoutX="49.0" layoutY="111.0" mnemonicParsing="false" onAction="#ajoutercategorie" prefHeight="25.0" prefWidth="89.0" text="Ajouter" />
